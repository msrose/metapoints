{
  "schemas": {
    "people": {
      "required": ["ip", "name"],
      "optional": {
        "metapoints": 10000,
        "powerLevel": 0,
        "active": 0,
        "authQuestion": null,
        "timeout": 0,
        "lastUpdatedBy": null,
        "multiplier": 1,
        "luck": 1,
        "decorations": false
      },
      "persist": ["ip", "name", "metapoints", "powerLevel", "lastUpdatedBy", "multiplier", "luck", "decorations"]
    },
    "messages": {
      "required": ["sender", "text", "time"]
    }
  }
}
